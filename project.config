[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group rdo-ci-group
	owner = group user/mathieu bultel (matbu)
[access "refs/for/refs/*"]
	read = group rdo-ci-group
	read = group user/mathieu bultel (matbu)
	push = group rdo-ci-group
	push = group user/mathieu bultel (matbu)
	label-Code-Review = -2..+2 group rdo-ci-group
	label-Code-Review = -2..+2 group user/mathieu bultel (matbu)
	label-Verified = -1..+1 group rdo-ci-group
	label-Verified = -1..+1 group user/mathieu bultel (matbu)
	rebase = group rdo-ci-group
	rebase = group user/mathieu bultel (matbu)
	removeReviewer = group rdo-ci-group
	removeReviewer = group user/mathieu bultel (matbu)
	submit = group rdo-ci-group
	submit = group user/mathieu bultel (matbu)
[access "refs/heads/*"]
	read = group rdo-ci-group
	read = group user/mathieu bultel (matbu)
	create = group rdo-ci-group
	create = group user/mathieu bultel (matbu)
	push = group rdo-ci-group
	push = +force group user/mathieu bultel (matbu)
	pushMerge = group rdo-ci-group
	pushMerge = group user/mathieu bultel (matbu)
[access "refs/tags/*"]
	push = group rdo-ci-group
	push = group user/mathieu bultel (matbu)
	push = +force group user/mathieu bultel (matbu)
